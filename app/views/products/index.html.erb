<p style="color: green"><%= notice %></p>

<h1>Products</h1>

<%= form_with url: "products/upload", multipart: true do |form| %>
  <%= file_field_tag :csv %>
  <%= form.submit %>
<% end %>
 
<div id="products">
  <% @products.each do |product| %>
    <%= render product %>
    <p>
      <%= link_to "Show this product", product %>
    </p>
  <% end %>
</div>

<%= link_to "New product", new_product_path %>
